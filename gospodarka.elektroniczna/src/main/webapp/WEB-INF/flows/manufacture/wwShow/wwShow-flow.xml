<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" start-state="wwShowView"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<!-- Zabezpieczenie flow przez Spring Security -->
	<secured attributes="ROLE_MANUFACTURE"/>

	<on-start>
		<evaluate expression="controllerVar.getWWDocuments()" result="flowScope.wwdocuments" />
	</on-start>


	<view-state id="wwShowView">
		<transition on="wwShow" to="wwShow" />
	</view-state>

	<bean-import resource="flow-beans.xml" />
</flow>
